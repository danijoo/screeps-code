{
  "extends": "tslint:recommended",
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "no-namespace": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}